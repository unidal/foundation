<?xml version="1.0" encoding="utf-8"?>
<class name="org.unidal.agent.cat.sample.Snoop">
   <method name="&lt;init&gt;" desc="()V">
      <transaction type="Snoop" name="${method}">
      </transaction>
   </method>
   <method name="snoop" desc="()Ljava/lang/String;">
      <transaction type="Snoop" name="${method}">
         <key>return</key>
         <value>${return}</value>
      </transaction>
      <event type="Snoop" name="${method}">
         <key>return</key>
         <value>${return}</value>
      </event>
   </method>
</class>