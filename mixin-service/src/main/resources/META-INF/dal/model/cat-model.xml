<?xml version="1.0" encoding="UTF-8"?>
<model model-package="org.unidal.agent.cat.model" class-suffix="Model" enable-sax-parser="true" enable-base-visitor="true">
  <entity name="root" root="true">
    <entity-ref name="class" type="map" names="classes" method-find-or-create="true" />
  </entity>
  <entity name="class">
    <attribute name="name" value-type="String" key="true" />
    <entity-ref name="method" type="list" names="methods" method-find-or-create="true" />
  </entity>
  <entity name="method">
    <attribute name="name" value-type="String" key="true" />
    <attribute name="desc" value-type="String" key="true" />
    <entity-ref name="transaction" />
    <entity-ref name="event" />
  </entity>
  <entity name="transaction">
    <attribute name="type" value-type="String" />
    <attribute name="name" value-type="String" />
  </entity>
  <entity name="event">
    <attribute name="type" value-type="String" />
    <attribute name="name" value-type="String" />
  </entity>
</model>

